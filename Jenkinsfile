pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                //
                git 'https://github.com/madhavith/pipeline_jenkins_feb.git'
                
                  }
                           }
        stage('Build') {
            steps {
                //
                sh 'mvn clean'
                  }
                        } 
            }  
         } 
